(defvar el-fetch-lib
  (expand-file-name
   "../../src/el-fetch/" (file-name-directory (nth 2 command-line-args)))
  "Path to the `el-fetch' library.")

(add-to-list 'load-path el-fetch-lib)

(require 'el-fetch nil t)


(el-fetch-console)
